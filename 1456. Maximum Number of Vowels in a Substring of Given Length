class Solution {
    public int maxVowels(String s, int k) {
        int i = 0;
        String vowel = "aeiou";
        while (i < k - 1){
            char ch = s.charAt[i];
            if (vowel.indexOf(ch) == -1){
                i++;
            }else{
                count ++;
                i++;
            }
        }
        int max_count = count; 
        int n = word.length();
        while (i + k < n-1){
            int temp_i = i;
            while (i < temp_i + k){
                char ch = s.charAt[i];
            if (vowel.indexOf(ch) == -1){
                i++;
            }else{
                count ++;
                i++;
            }
        }
        max_count = Math.max(count, max_count)
        }
    return max_count; 
    }
}
Corrected by GPT:
class Solution {
    public int maxVowels(String s, int k) {
        String vowel = "aeiou";
        int count = 0;
        for (int i = 0; i < k; i++){
            char ch = s.charAt(i);
            if (vowel.indexOf(ch) != -1){
                count ++;
            }
       }
       int max_count = count;
       int n = s.length();
       for (int i = k; i < n; i++){
            if (vowel.indexOf(s.charAt(i-k)) != -1){
                count--;
            }
            if(vowel.indexOf(s.charAt(i)) != -1){
                count++;
            }
            max_count = Math.max(count, max_count); 
       }
    return max_count; 
    }
}
